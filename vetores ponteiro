#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#define TAM 4
#define MX 10;

int main(){

    srand(time(NULL));
    
    char x[TAM];
    int v[TAM];
    
    //popular
    for(int k =0; k<TAM; k++){
        *(x+k) = rand()%MX;
        *(v+k) = rand()%MX;
    }
    //imprimir primeiro vetor
    for(int k=0; k<TAM; k++){
        printf("[%p] %d\n", x+k, *(x+k));
    }
    puts("==========================");
    //imprimir primeiro vetor
    for(int k=0; k<TAM; k++){
        printf("[%p] %d\n", v+k, *(v+k));
    }
return 0;
}
